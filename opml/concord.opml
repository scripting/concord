			<outline text="$(document).on(&quot;click&quot;, concord.updateFocusRootEvent);"></outline>
			<outline text="$(document).on(&quot;dblclick&quot;, concord.updateFocusRootEvent);"></outline>
			<outline text="$(document).on('show', function(e){">
