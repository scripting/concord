								</outline>
							<outline text="concordInstance.op.markChanged();"></outline>
							<outline text="}"></outline>
							</outline>
						<outline text="}"></outline>
						</outline>
					<outline text="}"></outline>
					</outline>
				<outline text="});"></outline>
				</outline>
			<outline text="$(document).on(&quot;mouseup&quot;, function(event) {">
				<outline text="if(!concord.handleEvents){">
